doctype html
html
    head
        meta(charset='utf-8')
        title Personal Area
        // BOOTSTRAP
        script(src='http://code.jquery.com/jquery-latest.js')
        script(src='scripts/jquery-1.9.1.js')
        script(src='scripts/bootstrap.js')
        link(href='stylesheets/bootstrap.min.css', rel='stylesheet')
        script(src='scripts/modernizr.custom.js')
        link(rel='stylesheet', type='text/css', href='stylesheets/normalize.css')
        link(rel='stylesheet', type='text/css', href='stylesheets/component.css')
        link(rel='stylesheet', type='text/css', href='stylesheets/content.css')
        // BOOTSTRAP END
        link(href='stylesheets/styletest1.css', rel='stylesheet')
    body
        #send_tab.tab-pane
            #accordion1.accordion
                for discount in discounts
                    .accordion-group.discount
                        .accordion-heading
                            a.accordion-toggle(width='100px', data-toggle='collapse', data-parent='#accordion1', href='#collapse_send_'+discount.id)
                                .heading Discount 1
                            .accordion-body.collapse.describe_bg(id='collapse_send_'+discount.id)
                                .accordion-inner.describe
                                    .describe_shadow
                                    .text
                                        | #{discount.userId}
                                    .btn
                                        a(href='#')
                                            .b_text Send
                                        .describe_shadow_down